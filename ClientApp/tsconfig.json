{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "target": "es5",
    "removeComments": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "esModuleInterop": true,
    "module": "esnext"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "src"
  ]
}
